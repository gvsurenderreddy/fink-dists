PatchScript: <<
	grep -Rl /usr/bin * |xargs perl -pi -e "s,/usr/bin,%p/bin,g"
	grep -Rl /usr/share * |xargs perl -pi -e "s,/usr/share,%p/share,g"
<<
CompileScript: <<
	%p/lib/qt4-x11/bin/qmake mediaarchiver.pro
	make
<<
